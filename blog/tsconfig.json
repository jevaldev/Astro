{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@comp/*": ["./src/components/*"],
      "@layout/*": ["./src/layouts/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}